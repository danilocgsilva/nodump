{
    "name": "danilocgsilva/nodump_database",
    "description": "Resource to clone database data without dump script - only read access",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Danilocgsilva\\NodumpDatabase\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Danilocgsilva\\Nodump\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Danilo Silva",
            "email": "contact@danilocgsilva.me"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^11.4"
    },
    "require": {
        "doctrine/migrations": "^3.8",
        "doctrine/orm": "^3.3",
        "symfony/cache": "^7.1"
    }
}
